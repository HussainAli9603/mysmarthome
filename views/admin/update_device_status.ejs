     <!-- Content Wrapper. Contains page content -->
     <div class="content-wrapper" style="margin: auto;">
        <!-- Content Header (Page header) -->
        <!-- Main content -->
        <section class="content">
    
          <!-- Default box -->
          <div class="card">
            <div class="card-header">
              <h3 class="card-title">Update Device Status </h3>
    
              <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse" title="Collapse">
                  <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-tool" data-card-widget="remove" title="Remove">
                  <i class="fas fa-times"></i>
                </button>
              </div>
              
            </div>
             <% if(success){ %>
               <div class="container">
                  <div class="alert" style="background-color: #FFFFFF;color:blue">
                      <%= success %>
                       <a href="" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                  </div>
               </div>
            <% } %>
            <div class="card-body p-0">
              <div class="module-body table" style="overflow-x: scroll;">
                <table cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped  display"
                    width="" >
                    <thead>
                        <tr>
                            <th>
                                S#NO
                            </th>
                            <th>
                                Device Name
                            </th>
                            <th>
                                ON/OFF
                            </th>
                            <th>
                                OPEN / CLOSE
                            </th>
                            <th>
                                Temperature
                            </th>
                            <th>
                                Volume
                            </th>
                            <th>
                                Channel
                            </th>
                            <th>
                                Color
                            </th>
                            <th>
                              Update Status
                            </th>


                        </tr>
                    </thead>
                    <tbody>
                   <% var sr = 1; getDevice.forEach(function(Device){ %>
                      <tr>
                       <td><%= sr %></td>
                        <td><a><%= Device.name %></a></td>
                        <% if(Device.onOff == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.onOff %></a></td>
                        <% } %>
                        <% if(Device.openClose == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.openClose %></a></td>
                        <% } %>
                         <% if(Device.temperature == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.temperature %></a></td>
                         <% } %>
                         <% if(Device.volume == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.volume %></a></td>
                         <% } %>
                         <% if(Device.channel == "NotApplicable"){ %>
                        <td><a></a></td>
                        <% }else{ %>
                        <td><a><%= Device.channel %></a></td>
                         <% } %>

                        <td><a><%= Device.color %></a></td>
                     
                        <td class="project-actions text-right">
                            <a class="btn btn-info btn-sm" href="/update1-device-status/<%= Device.id %>">
                                <i class="fas fa-pencil-alt">
                                </i>
                                Update Status
                            </a>
                        </td>
                    </tr>
                  <% sr++ }); %>
                    </tbody>
                    <tfoot>
                        <tr>
                           <th>
                                S#NO
                            </th>
                            <th>
                                Device Name
                            </th>
                            <th>
                                ON/OFF
                            </th>
                            <th>
                                OPEN / CLOSE
                            </th>
                            <th>
                                Temperature
                            </th>
                            <th>
                                Volume
                            </th>
                            <th>
                                Channel
                            </th>
                            <th>
                                Color
                            </th>
                            <th>
                              Update Status
                            </th>
                        </tr>
                    </tfoot>
                </table>
            </div>
